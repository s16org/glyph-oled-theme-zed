{
  "name": "glyph-oled-theme",
  "type": "dark",
  "ui": {
    "activityBar.background": "#000000",
    "editor.background": "#000000",
    "editorGroup.border": "#000000",
    "editorGroup.dropBackground": "#000000",
    "editorGroup.emptyBackground": "#000000",
    "editorGroupHeader.noTabsBackground": "#000000",
    "editorGroupHeader.tabsBackground": "#000000",
    "editorIndentGuide.activeBackground1": "#000000",
    "menu.background": "#000000",
    "menu.border": "#424242",
    "sideBar.background": "#000000",
    "statusBar.background": "#000000",
    "tab.inactiveBackground": "#000000",
    "titleBar.activeBackground": "#000000",
    "titleBar.inactiveBackground": "#000000",
    "terminal.background": "#000000",
    "terminalCursor.background": "#000000",
    "terminalCursor.foreground": "#60A5FA",
    "panel.background": "#000000",
    "panel.border": "#1F2937",
    "scrollbarSlider.background": "#1F293740",
    "scrollbarSlider.hoverBackground": "#1F293760",
    "scrollbarSlider.activeBackground": "#1F293780",
    "editor.selectionBackground": "#60A5FA30",
    "editor.selectionHighlightBackground": "#60A5FA20",
    "editor.findMatchBackground": "#FBBF2460",
    "editor.findMatchHighlightBackground": "#FBBF2430",
    "editorLineNumber.foreground": "#374151",
    "editorLineNumber.activeForeground": "#9CA3AF",
    "editorGutter.background": "#000000",
    "editorBracketMatch.background": "#60A5FA40",
    "editorBracketMatch.border": "#60A5FA",
    "editorIndentGuide.background1": "#1F2937",
    "minimap.background": "#000000",
    "editorSuggestWidget.background": "#0F0F0F",
    "editorSuggestWidget.border": "#1F2937",
    "editorSuggestWidget.selectedBackground": "#1F2937",
    "editorError.foreground": "#EF4444",
    "editorWarning.foreground": "#FBBF24",
    "editorInfo.foreground": "#60A5FA",
    "editorHint.foreground": "#34D399",
    "editorError.border": "#EF444480",
    "editorWarning.border": "#FBBF2480",
    "editorWidget.border": "#1F2937",
    "editorHoverWidget.border": "#1F2937",
    "notifications.border": "#1F2937"
  },
  "syntax": [
    {
      "scope": ["comment", "comment.block.documentation", "comment.block.documentation.ts", "comment.block.documentation.js"],
      "foreground": "#6B7280",
      "fontStyle": "italic"
    },
    {
      "scope": ["keyword", "keyword.control", "storage", "storage.type", "storage.modifier"],
      "foreground": "#F472B6"
    },
    {
      "scope": ["string", "string.quoted", "string.template"],
      "foreground": "#34D399"
    },
    {
      "scope": ["constant.numeric", "constant.language.boolean", "constant.language.null", "constant.language.undefined"],
      "foreground": "#FBBF24"
    },
    {
      "scope": ["entity.name.function", "support.function", "meta.function-call.generic.python"],
      "foreground": "#60A5FA"
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.type", "support.class", "storage.type.cs", "storage.type.java", "storage.type.primitive", "entity.name.type.interface"],
      "foreground": "#A78BFA"
    },
    {
      "scope": ["variable", "variable.other", "meta.definition.variable.name", "support.variable"],
      "foreground": "#E5E7EB"
    },
    {
      "scope": ["variable.other.constant", "variable.other.enummember", "constant.other"],
      "foreground": "#FB7185"
    },
    {
      "scope": ["entity.name.tag", "entity.name.tag.jsx", "support.class.component.jsx"],
      "foreground": "#FB7185"
    },
    {
      "scope": ["entity.other.attribute-name", "entity.other.attribute-name.jsx"],
      "foreground": "#FBBF24"
    },
    {
      "scope": ["punctuation.definition.tag", "punctuation.definition.tag.jsx"],
      "foreground": "#6B7280"
    },
    {
      "scope": ["punctuation.definition.string.begin", "punctuation.definition.string.end"],
      "foreground": "#34D399"
    },
    {
      "scope": ["punctuation.separator", "punctuation.terminator", "punctuation.accessor"],
      "foreground": "#9CA3AF"
    },
    {
      "scope": ["invalid"],
      "foreground": "#EF4444",
      "fontStyle": "bold"
    }
  ],
  "semantic": {
    "function": "#60A5FA",
    "method": "#60A5FA",
    "variable": "#E5E7EB",
    "parameter": "#FB7185",
    "property": "#60A5FA",
    "enumMember": "#FB7185",
    "interface": "#A78BFA",
    "type": "#A78BFA",
    "class": "#A78BFA",
    "namespace": "#A78BFA",
    "keyword": "#F472B6",
    "string": "#34D399",
    "number": "#FBBF24",
    "comment": "#6B7280",
    "decorator": "#FBBF24",
    "macro": "#F472B6",
    "struct": "#A78BFA",
    "enum": "#A78BFA",
    "typeParameter": "#A78BFA",
    "event": "#FB7185",
    "operator": "#E5E7EB",
    "regexp": "#34D399",
    "selfParameter": "#F472B6",
    "clsParameter": "#F472B6",
    "builtinType": "#A78BFA",
    "lifetime": "#FBBF24",
    "formatSpecifier": "#FBBF24",
    "generic": "#A78BFA",
    "unresolvedReference": "#EF4444"
  }
}

